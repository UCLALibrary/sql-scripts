'L3MPYRSY-2', '1CR107', 'L3MPYRSY-1', 'L3SPYRSY-1', 'L3SEYRSY-1', '1CR111', 'L3MPYRSP-2', '1CR119', 'L3MPYRWS-3', 'L3DEYRWS-2', 'L3MEYRWS-2', 'L3MPYRWS-2', '1CR118', 'L3SPYRWS-6',
'L3DEYRWS-1', 'L3MPYRWS-1', 'L3SPYRWS-1', 'L3SEYRWS-1', '1CR190', 'L3MPYRST03')

AND l.ledger_name = 'CRIS 16-17'


ORDER BY f.fund_code, b.title
;
